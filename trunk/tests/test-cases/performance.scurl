ssert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get "k0"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get "k19999"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}     },    {test-case "multi-key-hash-table-3-of",        def hash = {{MultiKeyHashTable-3-of String, String, String, String}                       "k1", "k2", "k3"                   }        def sw = {StopWatch}        ||attention!        ||約1分かかります。        {for i:int = 0 below 100000 do            {hash.set "k1-" & i, "k2-" & i, "k3-" & i, "aaa"}        }               {sw.start}        {hash.get "k1-50000", "k2-50000", "k3-50000"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get "k1-0", "k2-0", "k3-0"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get "k1-99999", "k2-99999", "k3-99999"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}    },    {test-case "multi-key-hash-table-of.from-key-name",        def hash = {{MultiKeyHashTable-3-of String, String, String, String}.from-key-name                       "k1", "k2", "k3"                   }        ||attention!        ||約1分かかります。        {for i:int = 0 below 100000 do            {hash.set "k1-" & i, "k2-" & i, "k3-" & i, "aaa"}        }        def sw = {StopWatch}        {sw.start}        {hash.get-from-key-name k1 = "k1-50000"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-0"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-99999"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}                {sw.reset}        {sw.start}        {hash.get-from-key-name k2 = "k2-50000"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k2 = "k2-0"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k2 = "k2-99999"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}                {sw.reset}        {sw.start}        {hash.get-from-key-name k3 = "k3-50000"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k3 = "k3-0"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k3 = "k3-99999"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}                {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-50000", k2 = "k2-50000"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-0", k2 = "k2-0"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-99999", k2 = "k2-99999"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}                {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-50000", k3 = "k3-50000"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-0", k3 = "k3-0"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-99999", k3 = "k3-99999"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}                {sw.reset}        {sw.start}        {hash.get-from-key-name k2 = "k2-50000", k3 = "k3-50000"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k2 = "k2-0", k3 = "k3-0"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k2 = "k2-99999", k3 = "k3-99999"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}                {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-50000", k2 = "k2-50000", k3 = "k3-50000"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-0", k2 = "k2-0", k3 = "k3-0"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}        {sw.reset}        {sw.start}        {hash.get-from-key-name k1 = "k1-99999", k2 = "k2-99999", k3 = "k3-99999"}        {sw.stop}        {dump {sw.elapsed}}        {TestCondition.assert {sw.elapsed} <= 5s}    }}