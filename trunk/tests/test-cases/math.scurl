{import * from COM.CURL.LIB.MATH}

{TestSuite "COM.CURL.LIB.MATH",
    {test-case "GCD",
        def (a,b,c) = (3,12,24)
        {TestCondition.assert
            {gcd a,b,c} == 3
        }


    },
    {test-case "LCM",
        def (a,b,c) = (2,3,12)
        {TestCondition.assert
            {lcm a,b,c} == 12
        }
    },
    {test-case "factorial",
        {TestCondition.assert {factorial 6} == 720}
    }
}

